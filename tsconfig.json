{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "outDir": "./static/js",
    "rootDir": "./static/ts",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "static/ts/**/*"
  ]
}
